<h1>List of All Active Questers</h1>


<ul>
    <%@users.each do |user|%>
            <img src="<%=user.image%>" width= "auto" height= "300px">
        <p><strong><%=link_to(user.username, user_path(user))%>
            </strong><%= button_to("Add Friend", relationships_path(:friend_id => user), :method => :post) %></p>
    <%end%>
</ul>